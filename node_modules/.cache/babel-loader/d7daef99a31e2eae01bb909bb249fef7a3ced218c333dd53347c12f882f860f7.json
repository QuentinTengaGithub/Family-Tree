{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { getAuth, signOut } from \"firebase/auth\";\nexport default {\n  computed: {\n    darkMode() {\n      return this.$store.state.darkMode;\n    },\n    user() {\n      return this.$store.state.user;\n    },\n    activeTab() {\n      const path = this.$route.path;\n      if (path.startsWith('/home')) return 'Home';\n      if (path.startsWith('/members')) return 'Members';\n      if (path.startsWith('/tree')) return 'Tree';\n      if (path.startsWith('/add-member')) return 'Accueil';\n      if (path.startsWith('/admin')) return 'Admin';\n      if (path.startsWith('/profile')) return 'Profile';\n      if (path.startsWith('/sign-up')) return 'SignUp';\n      if (path.startsWith('/sign-in')) return 'SignIn';\n      return 'Home'; // Fallback\n    }\n  },\n\n  methods: {\n    changeTab(tab) {\n      this.$router.push('/' + tab.toLowerCase());\n    },\n    toggleDarkMode() {\n      this.$store.commit('toggleDarkMode');\n    },\n    logout() {\n      const auth = getAuth();\n      signOut(auth).then(() => {\n        this.$router.push('/sign-in');\n      });\n    }\n  }\n};","map":{"version":3,"names":["getAuth","signOut","computed","darkMode","$store","state","user","activeTab","path","$route","startsWith","methods","changeTab","tab","$router","push","toLowerCase","toggleDarkMode","commit","logout","auth","then"],"sources":["/Users/quentin/Family-Tree/src/components/NavbarView.vue"],"sourcesContent":["<template>\n    <div>\n        <div class=\"navbar\" :class=\"{ dark: darkMode }\">\n            <div class=\"navbar-left\">\n                <!-- NAVBAR / LEFT ITEMS -->\n                <template v-if=\"user\">\n                    <div @click=\"changeTab('Home')\" class=\"navbar-items\">\n                        <img v-if=\"activeTab === 'Home' && darkMode\" src=\"../assets/home_fill_dark.png\" />\n                        <img v-else-if=\"activeTab === 'Home' && !darkMode\" src=\"../assets/home_fill.png\" />\n                        <img v-else-if=\"activeTab !== 'Home' && darkMode\" src=\"../assets/home_dark.png\" />\n                        <img v-else src=\"../assets/home.png\" />\n                    </div>\n                    <div @click=\"changeTab('Add-Member')\" :class=\"{ active: activeTab === 'Accueil', dark: darkMode }\" class=\"navbar-items\">Home</div>\n                    <div @click=\"changeTab('Tree')\" :class=\"{ active: activeTab === 'Tree', dark: darkMode }\" class=\"navbar-items\">Tree</div>\n                    <div @click=\"changeTab('Members')\" :class=\"{ active: activeTab === 'Members', dark: darkMode }\" class=\"navbar-items\">Members</div>\n                    <div v-if=\"user && user.role === 'admin'\" @click=\"changeTab('Admin')\" :class=\"{ active: activeTab === 'Admin', dark: darkMode }\" class=\"navbar-items\">Admin</div>\n                </template>\n            </div>\n\n            <!-- NAVBAR / RIGHT ITEMS -->\n            <div class=\"navbar-right\">\n                <div @click=\"toggleDarkMode\" class=\"navbar-items sun\">\n                    <img v-if=\"darkMode\" src=\"../assets/moon.png\" title=\"DARK MODE\" />\n                    <img v-else src=\"../assets/sun.png\" title=\"LIGHT MODE\" />\n                </div>\n                \n                <template v-if=\"user\">\n                    <div @click=\"changeTab('Profile')\" class=\"profile-container\">\n                        <img v-if=\"activeTab === 'Profile' && darkMode\" src=\"../assets/account_fill_dark.png\" class=\"profile-icon\" />\n                        <img v-else-if=\"activeTab === 'Profile' && !darkMode\" src=\"../assets/account_fill.png\" class=\"profile-icon\" />\n                        <img v-else-if=\"darkMode\" src=\"../assets/account_dark.png\" class=\"profile-icon\" />\n                        <img v-else src=\"../assets/account.png\" class=\"profile-icon\" />\n                        <span class=\"profile-text\">My Profile</span>\n                    </div>\n                    <div @click=\"logout\" class=\"navbar-items logout-container\">\n                        <img :src=\"darkMode ? require('../assets/logout_dark.png') : require('../assets/logout.png')\" alt=\"Logout Icon\" class=\"logout-icon\" />\n                        <span class=\"logout-text\" v-if=\"!darkMode\">Logout</span>\n                        <span class=\"logout-text\" style=\"color:white\" v-if=\"darkMode\">Logout</span>\n                    </div>\n                </template>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nimport { getAuth, signOut } from \"firebase/auth\";\n\nexport default {\n    computed: {\n        darkMode() {\n            return this.$store.state.darkMode;\n        },\n        user() {\n            return this.$store.state.user;\n        },\n        activeTab() {\n            const path = this.$route.path;\n            if (path.startsWith('/home')) return 'Home';\n            if (path.startsWith('/members')) return 'Members';\n            if (path.startsWith('/tree')) return 'Tree';\n            if (path.startsWith('/add-member')) return 'Accueil';\n            if (path.startsWith('/admin')) return 'Admin';\n            if (path.startsWith('/profile')) return 'Profile';\n            if (path.startsWith('/sign-up')) return 'SignUp';\n            if (path.startsWith('/sign-in')) return 'SignIn';\n            return 'Home'; // Fallback\n        }\n    },\n    methods: {\n        changeTab(tab) {\n            this.$router.push('/' + tab.toLowerCase());\n        },\n        toggleDarkMode() {\n            this.$store.commit('toggleDarkMode');\n        },\n        logout() {\n            const auth = getAuth();\n            signOut(auth).then(() => {\n                this.$router.push('/sign-in');\n            });\n        }\n    }\n}\n</script>\n\n<style>\n.navbar {\n    background-color: #fff; \n    display: flex; \n    flex-direction: horizontal;\n    border-bottom:solid 1px var(--lightGrey);\n    width: 100vw;\n    position:fixed;\n    top: 0;\n    left: 0;\n    z-index: 1000;\n    margin: 0 !important;\n    padding: 4px;\n    justify-content: space-between;\n}\n\n.navbar.dark {\n    background-color:black;\n}\n\n.navbar-items {\n    display: flex; \n    align-items: center;\n    margin-right: 10px;\n    color: black;\n    cursor: pointer;\n    padding:5px 15px;\n    width:fit-content;\n    border-radius: 10px;\n}\n\n.navbar-items.dark {\n    color:white;\n}\n\n.navbar-items:hover {\n    background-color: var(--lightGrey);\n}\n\n.navbar-items.active {\n    font-weight: bold;\n    color:black;\n}\n\n.navbar-items.dark.active {\n    color:white;\n}\n\n.navbar-items:last-child {\n    margin-left: auto;\n    margin-right: 20px;\n}\n\n.navbar-left,\n.navbar-right {\n  display: flex;\n  align-items: center;\n}\n\n.profile-container {\n    display: flex;\n    align-items: center;\n    cursor: pointer;\n    padding: 5px 15px;\n    border-radius: 10px;\n    margin-right: 10px;\n    transition: background-color 0.3s ease;\n}\n\n.profile-container:hover {\n    background-color: var(--lightGrey);\n}\n\n.profile-icon {\n    width: 20px;\n    height: 20px;\n    margin-right: 5px; /* Space between icon and text */\n}\n\n.profile-text {\n    max-width: 0;\n    overflow: hidden;\n    white-space: nowrap;\n    transition: max-width 0.3s ease-in-out, opacity 0.3s ease-in-out;\n    opacity: 0;\n    color: black; /* Default color */\n}\n\n.profile-container.dark .profile-text {\n    color: white;\n}\n\n.logout-container {\n    display: flex;\n    align-items: center;\n    cursor: pointer;\n    padding: 5px 15px;\n    border-radius: 10px;\n    margin-right: 10px;\n    transition: background-color 0.3s ease;\n}\n\n.logout-container:hover {\n    background-color: var(--lightGrey);\n}\n\n.logout-icon {\n    width: 20px;\n    height: 20px;\n    margin-right: 5px; /* Space between icon and text */\n}\n\n.logout-text {\n    max-width: 0;\n    overflow: hidden;\n    white-space: nowrap;\n    transition: max-width 0.3s ease-in-out, opacity 0.3s ease-in-out;\n    opacity: 0;\n    color: black; /* Default color */\n}\n\n.logout-container:hover .logout-text {\n    max-width: 100px; /* Adjust as needed */\n    opacity: 1;\n}\n\n.logout-container.dark .logout-text {\n    color: white;\n}\n</style>"],"mappings":";AA8CA,SAASA,OAAO,EAAEC,OAAM,QAAS,eAAe;AAEhD,eAAe;EACXC,QAAQ,EAAE;IACNC,QAAQA,CAAA,EAAG;MACP,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACF,QAAQ;IACrC,CAAC;IACDG,IAAIA,CAAA,EAAG;MACH,OAAO,IAAI,CAACF,MAAM,CAACC,KAAK,CAACC,IAAI;IACjC,CAAC;IACDC,SAASA,CAAA,EAAG;MACR,MAAMC,IAAG,GAAI,IAAI,CAACC,MAAM,CAACD,IAAI;MAC7B,IAAIA,IAAI,CAACE,UAAU,CAAC,OAAO,CAAC,EAAE,OAAO,MAAM;MAC3C,IAAIF,IAAI,CAACE,UAAU,CAAC,UAAU,CAAC,EAAE,OAAO,SAAS;MACjD,IAAIF,IAAI,CAACE,UAAU,CAAC,OAAO,CAAC,EAAE,OAAO,MAAM;MAC3C,IAAIF,IAAI,CAACE,UAAU,CAAC,aAAa,CAAC,EAAE,OAAO,SAAS;MACpD,IAAIF,IAAI,CAACE,UAAU,CAAC,QAAQ,CAAC,EAAE,OAAO,OAAO;MAC7C,IAAIF,IAAI,CAACE,UAAU,CAAC,UAAU,CAAC,EAAE,OAAO,SAAS;MACjD,IAAIF,IAAI,CAACE,UAAU,CAAC,UAAU,CAAC,EAAE,OAAO,QAAQ;MAChD,IAAIF,IAAI,CAACE,UAAU,CAAC,UAAU,CAAC,EAAE,OAAO,QAAQ;MAChD,OAAO,MAAM,EAAE;IACnB;EACJ,CAAC;;EACDC,OAAO,EAAE;IACLC,SAASA,CAACC,GAAG,EAAE;MACX,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,GAAE,GAAIF,GAAG,CAACG,WAAW,CAAC,CAAC,CAAC;IAC9C,CAAC;IACDC,cAAcA,CAAA,EAAG;MACb,IAAI,CAACb,MAAM,CAACc,MAAM,CAAC,gBAAgB,CAAC;IACxC,CAAC;IACDC,MAAMA,CAAA,EAAG;MACL,MAAMC,IAAG,GAAIpB,OAAO,CAAC,CAAC;MACtBC,OAAO,CAACmB,IAAI,CAAC,CAACC,IAAI,CAAC,MAAM;QACrB,IAAI,CAACP,OAAO,CAACC,IAAI,CAAC,UAAU,CAAC;MACjC,CAAC,CAAC;IACN;EACJ;AACJ"},"metadata":{},"sourceType":"module","externalDependencies":[]}